@model List<Course>

<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
</head>
<body>
    <h1> Course List Page</h1>

    @foreach (var course in Model)
    {
        <div id="kurs_@course.Id">

            <h2>Course Name: @course.Title</h2>
            <p>Desription: @course.Description</p>
            <img src="~/img/@course.Image" width="200"/>

        </div>
    }
</body>
</html>
